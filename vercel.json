{
    "version":2,
    "builds": [
        {
            "src":"StressDetection/wsgi.py",
            "use":"@vercel/python",
            "config":{"maxLambdaSize":"15mb", "runtime":"python3.7.2"}
        },
        {
            "src":"build_files.sh",
            "use":"@vercel/static-build",
            "config":{
                "distDir":"staticfiles_build"
            }
        }
    ],
    "routes": [
        {
            "src":"/static/(/*)",
            "dest":"/static/$1"
        },
        {
            "src":"/(.*)",
            "dest":"StressDetection/wsgi.py"
        }
    ]
}